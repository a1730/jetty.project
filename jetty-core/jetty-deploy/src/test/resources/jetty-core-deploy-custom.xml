<?xml version="1.0"?><!DOCTYPE Configure PUBLIC "-//Jetty//Configure//EN" "https://jetty.org/configure_10_0.dtd">

<Configure id="contextProvider" class="org.eclipse.jetty.deploy.providers.DefaultProvider">
  <Call id="coreEnv" class="org.eclipse.jetty.util.component.Environment" name="ensure">
    <Arg>core</Arg>
  </Call>
  <Call name="configureEnvironment">
    <Arg>core</Arg>
    <Set name="contextHandlerClass">
      <Property name="contextHandlerClass" default="org.eclipse.jetty.server.handler.ResourceHandler$ResourceContext" />
    </Set>
    <Call name="loadPropertiesFromPathName">
      <Arg>
        <Property name="jetty.deploy.common.properties" />
      </Arg>
    </Call>
  </Call>
</Configure>
