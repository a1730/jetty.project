<?xml version="1.0"?><!DOCTYPE Configure PUBLIC "-//Jetty//Configure//EN" "https://jetty.org/configure_10_0.dtd">

<!-- =============================================================== -->
<!-- Configure the "core" environment deployment defaults            -->
<!-- =============================================================== -->
<Configure id="defaultProvider" class="org.eclipse.jetty.deploy.providers.DefaultProvider">
  <Call name="configureEnvironment">
    <Arg>core</Arg>
    <Set name="contextHandlerClass">
      <Property name="contextHandlerClass" default="org.eclipse.jetty.server.handler.CoreWebAppContext" />
    </Set>
  </Call>
</Configure>
